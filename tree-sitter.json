{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "fennel",
      "camelcase": "Fennel",
      "title": "Fennel",
      "scope": "source.fennel",
      "file-types": [
        "fnl"
      ],
      "injection-regex": "^fennel$",
      "class-name": "TreeSitterFennel"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "CC0-1.0",
    "description": "Fennel grammar for tree-sitter",
    "authors": [
      {
        "name": "Alexei Mozaidze",
        "email": "lexerpexer@proton.me",
        "url": "https://github.com/alexeimozaidze"
      }
    ],
    "links": {
      "repository": "https://github.com/0komo/tree-sitter-fennel"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
